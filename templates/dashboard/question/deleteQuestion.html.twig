<form method="post" action="{{ path('dashboard_question_delete', {'id': question.id}) }}" onsubmit="return confirm('Voulez vous vraiment supprimer ce question?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ question.id) }}">
    <button class="btn btn-danger btn-sm">Supprimer</button>
</form>
